{
  "name": "cucumber-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate project to run WebdriverIO tests with Cucumber",
  "homepage": "https://github.com/webdriverio/cucumber-boilerplate",
  "main": "test/runner.js",
  "scripts": {
    "local-webserver": "http-server ./demo-app",
    "test": "babel-node ./node_modules/.bin/isparta cover _mocha --include-all-sources --default-excludes ['*/node_modules/**', '**/unit-tests/**'] --excludes '**/steps/*.js' --root test -- unit-tests --recursive"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/webdriverio/cucumber-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/webdriverio/cucumber-boilerplate/issues"
  },
  "keywords": [
    "webdriverio",
    "cucumber",
    "test",
    "selenium"
  ],
  "author": "Christian Bromann <christian@saucelabs.com>",
  "license": "MIT",
  "dependencies": {
    "babel-preset-es2015": "~6.14.0",
    "babel-register": "~6.14.0",
    "chai": "~3.5.0",
    "cucumber": "~1.3.1",
    "wdio-cucumber-framework": "^0.2.13",
    "wdio-phantomjs-service": "^0.2.2",
    "wdio-selenium-standalone-service": "^0.0.7",
    "wdio-spec-reporter": "^0.0.3",
    "webdriverio": "^4.5.0"
  },
  "devDependencies": {
    "eslint": "~3.6.0",
    "eslint-config-airbnb-base": "~5.0.3",
    "http-server": "~0.9.0",
    "isparta": "~4.0.0",
    "istanbul": "~0.4.5",
    "mocha": "~3.0.2",
    "sinon": "~1.17.5"
  }
}
